{
  "name": "@htmldocs/babel-plugin-webpack",
  "version": "0.0.0",
  "private": true,
  "peerDependencies": {
    "react": "^18.2.0"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
