    "@htmldocs/eslint-config": "workspace:*",
    "@htmldocs/typescript-config": "workspace:*",
    "@types/babel__generator": "^7.6.8",
    "@types/babel__traverse": "^7.20.5",
    "@types/eslint": "^8.56.5",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.11.24",
    "@types/react": "^18.2.61",
    "@types/react-dom": "^18.2.19",
    "eslint": "^8.57.0",
    "fs-extra": "^11.2.0",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5",
    "webpack": "^5.91.0"
  }
